# ===========================================
# AgentCostControl Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ===========================================
# Supabase (Database)
# ===========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ===========================================
# Upstash Redis (Cache)
# ===========================================
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token-here

# ===========================================
# LLM Provider API Keys
# ===========================================
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...
GOOGLE_API_KEY=...
DEEPSEEK_API_KEY=...
XAI_API_KEY=...
MISTRAL_API_KEY=...

# ===========================================
# Authentication
# ===========================================
JWT_SECRET_KEY=generate-a-random-string-at-least-32-characters-long
API_KEY_SALT=another-random-string-for-hashing

# ===========================================
# Payments (Stripe)
# ===========================================
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...

# ===========================================
# Email (Resend)
# ===========================================
RESEND_API_KEY=re_...

# ===========================================
# Monitoring (Sentry)
# ===========================================
SENTRY_DSN=https://xxx@sentry.io/xxx

# ===========================================
# App Configuration
# ===========================================
ENVIRONMENT=development
LOG_LEVEL=INFO

# ===========================================
# Optional: ClawHub Integration
# ===========================================
CLAWHUB_API_KEY=optional-for-skill-scanning
