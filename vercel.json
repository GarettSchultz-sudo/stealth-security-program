{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/v1/messages",
      "dest": "/api/proxy/anthropic.py"
    },
    {
      "src": "/v1/chat/completions",
      "dest": "/api/proxy/openai.py"
    },
    {
      "src": "/health",
      "dest": "/api/proxy/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "api/proxy"
  }
}
